           <widget class="QLabel" name="TempoTextLabel">
            <property name="minimumSize">
             <size>
              <width>40</width>
              <height>0</height>
             </size>
            </property>
            <property name="text">
             <string>&amp;Tempo:</string>
            </property>
            <property name="alignment">
             <set>Qt::AlignRight|Qt::AlignTrailing|Qt::AlignVCenter</set>
            </property>
            <property name="buddy">
             <cstring>TempoSpinBox</cstring>
            </property>
           </widget>
          </item>
          <item row="0" column="1" colspan="2">
           <widget class="qtractorTempoSpinBox" name="TempoSpinBox">
            <property name="minimumSize">
             <size>
              <width>80</width>
              <height>0</height>
             </size>
            </property>
            <property name="toolTip">
             <string>Tempo (BPM) / Signature</string>
            </property>
           </widget>
          </item>
          <item row="1" column="1">
           <widget class="QPushButton" name="TempoTapPushButton">
            <property name="text">
             <string>T&amp;ap</string>
            </property>
           </widget>
          </item>
          <item row="1" column="2" colspan="2">
           <spacer>
            <property name="orientation">
             <enum>Qt::Horizontal</enum>
            </property>
            <property name="sizeHint">
             <size>
              <width>20</width>
              <height>20</height>
             </size>
            </property>
           </spacer>
          </item>
         </layout>
        </widget>
       </item>
       <item>
        <widget class="QGroupBox" name="MarkerGroupBox">
         <property name="title">
          <string>Marker</string>
         </property>
         <layout class="QGridLayout">
          <item row="0" column="0" colspan="2">
           <widget class="QLineEdit" name="MarkerTextLineEdit">
            <property name="toolTip">
             <string>Marker text</string>
            </property>
           </widget>
          </item>
          <item row="0" column="3">
           <widget class="QToolButton" name="MarkerColorToolButton">
            <property name="minimumSize">
             <size>
              <width>24</width>
              <height>24</height>
             </size>
            </property>
            <property name="toolTip">
             <string>Marker color</string>
            </property>
            <property name="icon" >
             <iconset resource="qtractor.qrc" >:/images/formColor.png</iconset>
            </property>
            <property name="text">
             <string>...</string>
            </property>
           </widget>
          </item>
          <item row="1" column="0" colspan="4">
           <spacer>
            <property name="orientation">
             <enum>Qt::Vertical</enum>
            </property>
            <property name="sizeHint">
             <size>
              <width>20</width>
              <height>4</height>
             </size>
            </property>
           </spacer>
          </item>
         </layout>
        </widget>
       </item>
       <item>
        <widget class="QGroupBox" name="TempoFactorGroupBox">
         <property name="title">
          <string>Tempo scale factor</string>
         </property>
         <layout class="QGridLayout">
          <item row="0" column="1">
           <widget class="QDoubleSpinBox" name="TempoFactorSpinBox">
            <property name="minimumSize">
             <size>
              <width>40</width>
              <height>0</height>
             </size>
            </property>
            <property name="toolTip" >
             <string>Tempo scale factor</string>
            </property>
            <property name="accelerated">
             <bool>true</bool>
            </property>
            <property name="decimals" >
             <number>2</number>
            </property>
            <property name="minimum" >
             <double>0.01</double>
            </property>
            <property name="maximum" >
             <double>10.00</double>
            </property>
            <property name="singleStep" >
             <double>0.01</double>
            </property>
            <property name="value" >
             <double>1.00</double>
            </property>
           </widget>
          </item>
          <item row="0" column="2">
           <widget class="QPushButton" name="TempoFactorPushButton">
            <property name="text">
             <string>App&amp;ly</string>
            </property>
           </widget>
          </item>
         </layout>
        </widget>
       </item>
      </layout>
     </widget>
