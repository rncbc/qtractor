<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-qtractor-session">
    <sub-class-of type="text/xml"/>
    <comment>Qtractor session</comment>
    <glob pattern="*.qtr"/>
    <glob pattern="*.qts"/>
    <icon name="org.rncbc.qtractor.application-x-qtractor-session"/>
  </mime-type>
  <mime-type type="application/x-qtractor-template">
    <sub-class-of type="text/xml"/>
    <comment>Qtractor template</comment>
    <glob pattern="*.qtt"/>
    <icon name="org.rncbc.qtractor.application-x-qtractor-template"/>
  </mime-type>
  <mime-type type="application/x-qtractor-archive">
    <sub-class-of type="text/xml"/>
    <comment>Qtractor archive</comment>
    <glob pattern="*.qtz"/>
    <icon name="org.rncbc.qtractor.application-x-qtractor-archive"/>
  </mime-type>
</mime-info>
