# qtractor.pri
#
PREFIX  = @ac_prefix@
BINDIR  = @ac_bindir@
LIBDIR  = @ac_libdir@
DATADIR = @ac_datadir@
MANDIR  = @ac_mandir@

VERSION = @ac_version@
CONFIG += @ac_debug@

INCLUDEPATH += @ac_incpath@
LIBS += @ac_libs@

# Extra optimization flags
QMAKE_CXXFLAGS += @ac_cflags@
QMAKE_LFLAGS += @ac_ldflags@

unix:!macx {
	# X11 support
	QX11EXTRAS = @ac_qx11extras@
	!isEmpty(QX11EXTRAS) {
		QT += x11extras
	}
}
